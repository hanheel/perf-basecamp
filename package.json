{
  "name": "frontend-performance-basecamp",
  "version": "0.0.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "description": "frontend-performance-basecamp",
  "homepage": "https://hanheel.github.io/perf-basecamp",
  "main": "index.tsx",
  "scripts": {
    "start": "webpack serve --mode=development",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "build:ghpages": "webpack --mode=production --env deploy=ghpages",
    "build:cloudfront": "webpack --mode=production --env deploy=cloudfront",
    "postbuild:ghpages": "ts-node scripts/convert-images.js && ts-node scripts/convert-gif.js",
    "postbuild:cloudfront": "ts-node scripts/convert-images.js && ts-node scripts/convert-gif.js",
    "watch": "webpack --watch",
    "serve": "webpack serve --mode=development",
    "prettier": "prettier --write .",
    "deploy": "npm run build:ghpages && npx gh-pages -d dist"
  },
  "keywords": [],
  "author": "woowacourse",
  "license": "MIT",
  "dependencies": {
    "@giphy/js-fetch-api": "^4.1.1",
    "@giphy/js-types": "^4.2.1",
    "classnames": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@types/node": "^18.7.13",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@webpack-cli/generators": "^2.2.0",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "22.0.0",
    "eslint-plugin-hooks": "^0.4.3",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-react": "^7.31.1",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "image-minimizer-webpack-plugin": "^4.1.4",
    "imagemin-webp": "^8.0.0",
    "mini-css-extract-plugin": "^2.9.4",
    "prettier": "^2.3.2",
    "sharp": "^0.34.3",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "webpack": "^5.88.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  }
}
